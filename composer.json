{
    "name": "semplon/moota-sdk",
    "description": "PHP SDK for moota.co",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Moota",
            "homepage": "https://moota.co/"
        },
        {
            "name": "Aprilus Lumbantoruan",
            "email": "i@pilus.me"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "~1.0",
        "guzzlehttp/guzzle": "~6.2.2"
    },
    "require-dev": {
        "codeless/jugglecode": "1.0",
        "phpunit/phpunit": "~5.7.0",
        "satooshi/php-coveralls": "1.0",
        "vlucas/phpdotenv": "~2.2"
    },
    "autoload": {
        "files": [
            "helpers.php",
            "constants.php"
        ],
        "psr-4": {
            "Moota\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moota\\SDK\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
